; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="A Spell Called Gun"
run/main_scene="res://Project/Main Menu/Scenes/main_menu.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

MenuTemplateManager="*res://Project/Main Menu/Nodes/menu_template_manager.tscn"
GlobalAscgFunc="*res://Project/Singelton/GlobalASCGFunc.gd"
GlobalSteamApi="*res://Project/Singelton/GlobalSteamAPI.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=480
window/size/mode=3
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/ColorPreview/plugin.cfg", "res://addons/SimpleTODO/plugin.cfg")

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[godot_idea_board]

directory_path="res://addons/godot-idea-board/savedata/"
